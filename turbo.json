{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "inputs": ["./src/**"],
      "outputs": ["./dist"],
      "dependsOn": ["^build"]
    },
    "build:docker": {
      "cache": false
    },
    "push:docker": {
      "cache": false
    },
    "test": {
      "inputs": ["./src/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    }
  }
}
