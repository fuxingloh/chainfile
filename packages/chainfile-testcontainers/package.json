{
  "name": "chainfile-testcontainers",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "url": "git+https://github.com/vetumorg/chainfile"
  },
  "license": "MIT",
  "files": [
    "**/*.d.ts",
    "**/*.d.ts.map",
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "tsc --build --clean",
