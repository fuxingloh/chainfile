      "prettier --write"
    ]
  },
  "jest": {
    "preset": "@workspace/jest-preset"
  },
  "dependencies": {
    "chainfile": "workspace:^",
    "chainfile-docker": "workspace:^",
    "testcontainers": "^10.9.0"
  }
