{
  "name": "chainfile-docker",
  "version": "0.0.0",
  "private": false,
  "repository": {
    "url": "git+https://github.com/vetumorg/chainfile"
  },
  "license": "MIT",
  "bin": {
    "chainfile-docker": "./bin.js"
  },
  "files": [
    "**/*.d.ts",
    "**/*.d.ts.map",
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "tsc --build --clean",
    "lint": "eslint .",
    "test": "jest"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "@workspace/jest-preset"
  },
  "dependencies": {
    "chainfile": "workspace:^",
    "clipanion": "3.2.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@workspace/jest-preset": "workspace:*",
    "@workspace/tsconfig": "workspace:*"
  }
}
